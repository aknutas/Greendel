    	<Style x:Key="LegendStyle1" TargetType="visualizationToolkit:Legend">
    		<Setter Property="Template">
    			<Setter.Value>
    				<ControlTemplate TargetType="visualizationToolkit:Legend">
    					<Grid Visibility="Collapsed" Width="0" Height="0"/>
    				</ControlTemplate>
    			</Setter.Value>
    		</Setter>
