    SupportedOrientations="Landscape" Orientation="Landscape"
    d:DesignHeight="480" d:DesignWidth="728"
    shell:SystemTray.IsVisible="True" Loaded="PhoneApplicationPage_Loaded" mc:Ignorable="d" BorderBrush="Black" Background="Black">
