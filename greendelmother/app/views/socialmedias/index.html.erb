<h1>Listing socialmedias</h1>

<table>
  <tr>
    <th>Status</th>
    <th>Facebook</th>
  </tr>

<% @socialmedias.each do |socialmedia| %>
  <tr>
    <td><%=h socialmedia.status %></td>
    <td><%=h socialmedia.facebookon %></td>
    <td><%= link_to 'Show', socialmedia %></td>
    <td><%= link_to 'Edit', edit_socialmedia_path(socialmedia) %></td>
    <td><%= link_to 'Destroy', socialmedia, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New socialmedia', new_socialmedia_path %>